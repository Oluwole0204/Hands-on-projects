numbers = [5, 2, 1, 7,  4]
numbers.append(10)
print(numbers)
numbers = [5, 2, 1, 7,  4]
numbers.insert(0,20)
print(numbers)
numbers = [5, 2, 1, 7,  4]
numbers.remove(5)
print(numbers)
numbers = [5, 2, 1, 7,  4]
numbers.sort()
print(numbers)
numbers = [5, 2, 1, 7,  4]
numbers.reverse()
print(numbers)
numbers = [5, 2, 1, 7,  4]
numbers2 = numbers.copy()
numbers.append(10)
print(numbers2)
numbers = [5,5, 2, 1, 7,  4]
print(numbers.count(5))
